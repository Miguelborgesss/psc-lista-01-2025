programa {
  funcao inicio() {
    cadeia nome
    inteiro idade
    real altura

    escreva("Digite seu nome:")
    leia(nome)

    escreva("Digite sua idade:")
    leia(idade)

    escreva("Digite sua altura (em cm):")
    leia(altura)

     escreva("\nSeu nome é:", nome  )
    escreva("\nSua idade é:", idade )
    escreva("\nSua altura é:", altura)
  }
}
